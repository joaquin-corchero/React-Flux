{
	"extends": "eslint:recommended",
	"rules": {
		"no-alert": 0,
		"no-bitwise": 0,
		"camelcase": 1,
		"curly": 1,
		"eqeqeq": 0,
		"no-eq-null": 0,
		"guard-for-in": 1,
		"no-empty": 1,
    "no-unused-vars": 0,
		"no-undef": 1,
	},
	"globals": {
    "jQuery": true,
    $: true
	},
	"env": {
		"browser": true,
		"node": true,
    "jquery": true
	}
}
